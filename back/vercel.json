{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/all-products",
        "dest": "/api/allProducts.js"
      },
      {
        "src": "/uploads",
        "dest": "/controllers/fetchImages.js"
      },
      {
        "src": "/api/all-messages",
        "dest": "/api/allMessages.js"
      }
    ]   
  }